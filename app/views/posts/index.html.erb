<p id="notice"><%= notice %></p>

<h1>All Posts</h1>

<table>
    <thead>
        <tr>
            <th>Posts</th>
        </tr>
    </thead>
    <tbody>
        <% @posts.each do |post| %>
          <td>
        <% if !current_user%>
        <%=  %>
        <% else %>
        <%= post.user.username %>
        <% end %>
        </td>
        <tr>
         <% if user_signed_in? %>
         <td><%= post.posts %></td>
        <% end %>
        <% end %>
    </tbody>
</table>